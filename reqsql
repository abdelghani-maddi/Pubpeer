/* importer la table globale de publications */
drop table "DATA".ROW_DATA;
create table "DATA".ROW_DATA 
(
	ID_EXTRACTION_COM		VARCHAR(150)	,
	ID		                VARCHAR(150)	,
	ORIGINAL_ID		        VARCHAR(150)	,
	PUBPEER_ID		        VARCHAR(50)	,
	PMID_PUBPEERMIRROR		VARCHAR(50)	,
	PMID_OPENALEX		    VARCHAR(50)	,
	PMCID_PUBPEERMIRROR		VARCHAR(50)	,
	PMCID_OPENALEX		    VARCHAR(50)	,
	OPENALEX_ID		        VARCHAR(50)	,
	URL_ARTICLE		        VARCHAR(250)	,
    DOI		                VARCHAR(250)	,
    TITRE		            VARCHAR(500)	,
    TITRE_SCRAPE		    VARCHAR(500)	,
    SOURCE		            VARCHAR(50)	,
    MAG_ID_SOURCE		    VARCHAR(50)	,
    JOURNAL_TITLE_WOS		VARCHAR(50)	,
    JOURNAL_CATEGORIES_WOS	VARCHAR(500)	,
    JOURNAL_AREAS_WOS		VARCHAR(500)	,
    JOURNAL_ESI_DISCIPLINES	VARCHAR(500)	,
    JOURNAL_DOMAINES		VARCHAR(500)	,
    DATE					VARCHAR(50)	,
    ANNEE					VARCHAR(50)	,
    TYPE					VARCHAR(50)	,
    OPEN_ACCESS				VARCHAR(50)	,
    RETRACTED				VARCHAR(50)	,
    MOTS_CLES				VARCHAR(50)	,
    MOTS_CLES_MESH			VARCHAR(50)	,
    RESUME_PUBPEERMIRROR	VARCHAR(500)	,
    RESUME_OPEN_ALEX		VARCHAR(500)	,
    AUTEURS					VARCHAR(500)	,
    INSTITUTIONS			VARCHAR(1500)	,
    INSTITUTION_TOTAL		VARCHAR(50)	,
    ID_ROR					VARCHAR(50)	,
    TYPE_INSTITUTION		VARCHAR(50)	,
    PAYS_INSTITUTION		VARCHAR(50)	,
    CITE					VARCHAR(50)	,
    CITE_ANNEE				VARCHAR(50)	,
    NOMBRE_DE_COMMENTAIRES	INTEGER,
    
	PRIMARY KEY (id)
)
;

COPY "DATA".ROW_DATA
FROM '/Users/maddi/Documents/Pubpeer/PubPeer_Base publications_postgreSQL2.csv'
DELIMITER ','
CSV HEADER;

